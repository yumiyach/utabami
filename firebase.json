{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/sitatametter",
        "destination": "https://sitatametter.utabami.com",
        "type": 301
      },
      {
        "source": "/sitatametter/**",
        "destination": "https://sitatametter.utabami.com",
        "type": 301
      },
      {
        "source": "/",
        "destination": "https://yumiya.utabami.com",
        "type": 301
      }
    ]
  }
}
